<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
        <link rel="stylesheet" href="/css/stylesheet.css">
        <title>The Battleship Game</title>
    </head>
    <body>
        <h1 class="text-center">The Battleship Game</h1><br>
        <div id="listGames">
          <div class="container">
            <div class="col-xs-6 alert alert-info col-xs-offset-3">
              <h3 class="text-center no-margin">Available games</h3>
              <div class="radio" v-for='element in gamesList'>
                <label><input type="radio" name="optradio" :value="element.name" v-model="picked">{{element.name}}</label>
              </div>
              <button class="btn btn-primary" id='button' v-on:click="Choose">Choose Game</button>
            </div>
          </div>

    </div>


      <!-- JS -->
    <script src="/node_modules/vue/dist/vue.js"></script>
    <script src="/node_modules/vue-resource/dist/vue-resource.js"></script>
    <script src="/socket.io/socket.io.js"></script>
    <script src="/js/app.js"></script>
    </body>
</html>
